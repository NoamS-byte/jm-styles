<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="note" version="1.0" default-locale="de-CH">
 <info>
  <title>JM Onlinekommentar Schweiz (Deutsch)</title>
  <id>https://juris-m.github.io/styles/jm-onlinekommentar-schweiz-(deutsch)</id>
  <author>
   <name>Noam Schank</name>
   <email>schank.noam@gmail.com</email>
  </author>
  <author>
   <name>Stephan Schlegel</name>
   <email>stephan.schlegel@unibas.ch</email>
   <note>Original author of the style "Juristische Zitierweise Schweizer (Ryser Büschi et al.) (Deutsch - Schweiz)" (https://www.zotero.org/styles/juristische-zitierweise-schweizer) which served as a template to this style.</note>
  </author>  
  <category citation-format="note"/>
  <category field="law"/>
  <summary>This style is based on the citation guidelines of Onlinekommentar (German edition).</summary>
  <updated>2026-01-27</updated>
  <rights license="https://creativecommons.org/licenses/by-sa/4.0/">This work is licensed under a Attribution-ShareAlike 4.0 International License.</rights>
    <!-- For this style, the author (Noam Schank) has written a manual (in German) which can be requested by e-mail. -->
 </info>
 <locale xml:lang="de">
  <style-options jurisdiction-preference="ch deAT LegCit"/>
  <terms>
   <term name="volume">Band</term>
   <term name="accessed">besucht am</term>
   <term name="editor">Hrsg.</term>
   <term name="et-al">et al.</term>
   <term name="page">S.</term>
   <term name="note">N.</term>
   <term name="paragraph">§</term>
   <term name="section">E.</term>
  </terms>
  <court-class name="echr" country="coe.int" courts="egmr"/>
  <court-class name="ecj" country="eu.int" courts="eugh"/>
  <court-class name="icj" country="un.int" courts="igh"/>
  <court-class name="b0" country="ch" courts="bger"/>
  <court-class name="b1" country="ch" courts="bvger bstger bpatger"/>
  <court-class name="k1" country="ch" courts="KGer OGer KassGer AppGer TC VGer"/>
  <court-class name="k2" country="ch" courts="BezGer TA ZMG TMC"/>
 </locale>
 <macro name="author">
  <names variable="author">
   <name delimiter="/" name-as-sort-order="all" sort-separator=" " form="long" font-style="normal"/>
   <label form="short" prefix=" (" suffix=")"/>
   <substitute>
    <names variable="editor"/>
   </substitute>
  </names>
 </macro>  
 <macro name="editor-author">
  <names variable="editor">
   <name delimiter="/" name-as-sort-order="all" sort-separator=" " form="long" font-style="normal"/>
   <label form="short" prefix=" (" suffix=")"/>
   <substitute>
    <names variable="author"/>
   </substitute>
  </names>
 </macro>
 <macro name="author-note">
  <names variable="author">
   <name form="short" delimiter="/" et-al-min="5" et-al-use-first="1" name-as-sort-order="all" font-style="normal"/>
  </names>
 </macro>
 <macro name="autor-editor-note">
  <names variable="author">
   <name form="short" delimiter="/" et-al-min="5" et-al-use-first="1" sort-separator="" font-style="normal"/>
   <substitute>
    <names variable="editor"/>
   </substitute>
  </names>
 </macro>
 <macro name="issued-date">
  <date variable="issued" prefix="vom ">
   <date-part name="day" suffix="."/>
   <date-part name="month" form="numeric" suffix="."/>
   <date-part name="year"/>
  </date>
 </macro>
 <macro name="issued-date-international-courts">
  <date variable="issued">
   <date-part name="day" suffix="."/>
   <date-part name="month" form="numeric" suffix="."/>
   <date-part name="year"/>
  </date>
 </macro>
 <macro name="issued-date-document">
  <date variable="issued">
   <date-part name="year"/>
  </date>
 </macro>
 <macro name="issued-date-hearing">
  <date variable="issued">
   <date-part name="year"/>
  </date>
 </macro>
 <macro name="accessed-date">
  <date variable="accessed">
   <date-part name="day" suffix="."/>
   <date-part name="month" form="numeric" suffix="."/>
   <date-part name="year"/>
  </date>
 </macro>
 <macro name="locator">
  <choose>
   <if locator="page">
    <group delimiter=" ">
     <text value="S."/>
     <text variable="locator"/>
    </group>
   </if>
   <else-if locator="note">
    <group delimiter=" ">
     <text value="N."/>
     <text variable="locator"/>
    </group>
   </else-if>
   <else-if locator="paragraph">
    <group delimiter=" ">
     <text value="§"/>
     <text variable="locator"/>
    </group>
   </else-if>
   <else-if locator="section">
    <group delimiter=" ">
     <text value="E."/>
     <text variable="locator"/>
    </group>
   </else-if>
   <else>
    <group delimiter=" ">
     <label variable="locator" form="short" suffix=" "/>
     <text variable="locator"/>
    </group>
   </else>
  </choose>
 </macro>
 <macro name="sort-by-category">
  <choose>
   <if type="document">
    <text value="1"/>    
   </if>
   <if type="hearing">
    <text value="2"/>
   </if>
   <if type="legal_case">
    <text value="3"/>
   </if>
   <else>
    <text value="4"/>
   </else>
  </choose>
 </macro>
 <macro name="sort-courts">
  <choose>
   <if country="coe.int">
    <text value="01"/>
   </if>
   <else-if country="eu.int">
    <text value="02"/>
   </else-if>
   <else-if country="un.int">
    <text value="03"/>
   </else-if>
   <else-if country="ch">
    <choose>
     <if court-class="b0" match="any">
      <text value="10"/>
     </if>
     <else-if court-class="b1">
      <text value="20"/>
     </else-if>
     <else-if court-class="k1" match="any">
      <text value="30"/>
     </else-if>
     <else-if court-class="k2" match="any">
      <text value="40"/>
     </else-if>
     <else>
      <text value="50"/>
     </else>
    </choose>
   </else-if>
  </choose> 
 </macro>
 <macro name="collection-title">
  <if variable="collection-title">
   <group delimiter=" ">
    <text variable="collection-title"/>
    <group delimiter=" ">
     <text term="volume"/>
     <number variable="collection-number"/>
    </group>
   </group>
  </if>     
 </macro>
 <citation>
  <layout delimiter="; ">
   <group suffix=".">
    <choose>
     <if type="article-journal">
      <group delimiter=", ">
       <text macro="author-note"/>
       <text variable="title-short"/>
       <text macro="locator"/>
      </group>
     </if>
     <else-if type="book">
      <group delimiter=", ">
       <text macro="autor-editor-note"/>
       <text variable="title-short"/>
       <text macro="locator"/>
      </group>
      </else-if>
      <else-if type="chapter">
       <group delimiter=", ">
        <text macro="author-note"/>
        <text variable="title-short"/>
        <text macro="locator"/>
       </group>
      </else-if>
      <else-if type="legal_commentary">
       <text variable="title-short"/>
       <text macro="locator"/>
      </else-if>        
      <else-if type="legal_case" match="any">
       <choose>
        <if type="legal_case" country="eu.int">
         <group delimiter=", ">
          <group delimiter=" ">
           <text value="EuGH"/>            
           <choose>
            <if variable="division">
             <text variable="division" prefix="(" suffix=")"/>
            </if>
           </choose>
          </group>
          <text variable="title" font-style="italic"/>
          <text variable="number" prefix="C-"/>
          <text variable="container-title"/>
          <text macro="issued-date-international-courts"/>            
          <text macro="locator"/>
         </group>
        </if>
        <else-if type="legal_case" country="coe.int">
         <group delimiter=", ">
          <group delimiter=" ">
           <text value="EGMR"/>
           <choose>
            <if variable="division">
             <text variable="division" prefix="(" suffix=")"/>
            </if>
           </choose>
          </group>
          <group delimiter=" ">
           <text variable="title" font-style="italic"/>
           <text variable="note" prefix="(" suffix=")"/>
          </group>
          <text variable="number" prefix="Nr. "/>
          <text macro="issued-date-international-courts"/>
          <text macro="locator"/>
         </group>
        </else-if>
        <else-if type="legal_case" country="un.int">
         <group delimiter=", ">
          <text value="IGH"/>
          <group delimiter=" ">
           <text variable="title" font-style="italic"/>
           <text variable="title-short" prefix="(" suffix=")"/>
          </group>
          <text variable="genre"/>
          <text macro="issued-date-international-courts"/>
          <text variable="container-title"/>
          <text macro="locator" prefix="(" suffix=")"/>
         </group>
        </else-if>
        <else-if type="legal_case" country="ch">
         <group delimiter=", ">
          <group delimiter=" ">
           <text variable="authority"/>
           <text variable="title"/>
           <text variable="number"/>
           <text macro="issued-date"/>
          </group>
          <text variable="container-title"/>
          <text variable="page" prefix="S. "/>
          <text macro="locator"/>
          <text variable="title-short" font-style="italic"/>
         </group>
        </else-if>
        <else>
         <group delimiter=", ">
          <if variable="title">
           <text variable="title"/>
          </if>
          <group delimiter=" ">
           <text variable="authority"/>
           <choose>
            <if match="any" variable="abstract">
             <text variable="number" suffix=", "/>
             <text variable="abstract"/>
            </if>
            <else>
             <text variable="number"/>
            </else>
           </choose>
           <text macro="issued-date"/>
          </group>
          <choose>
           <if match="any" variable="note">
            <text variable="note"/>
           </if>
          </choose>
          <choose>
           <if match="any" variable="title-short">
            <text variable="title-short" font-style="italic" prefix="&#8211; "/>
           </if>
          </choose>
         <text macro="locator"/>
        </group>
       </else>
      </choose>
     </else-if>
     <else-if type="document">
      <group delimiter=", ">
       <text variable="title-short"/>
       <text macro="issued-date-document"/>
       <text macro="locator"/>
      </group>
     </else-if>
     <else-if type="hearing">
      <group delimiter=" ">
       <text variable="title" suffix=", "/>
       <text variable="publisher"/>
       <text macro="issued-date-hearing"/>
       <text variable="authority"/>
       <text macro="locator"/>
      </group>
     </else-if>
     <else-if type="report">
      <group delimiter=", ">
       <text macro="author-note"/>
       <text variable="title-short"/>
       <text macro="locator"/>
      </group>
     </else-if>
     <else>
      <group delimiter=", ">
       <text macro="author-note"/>
       <text variable="title-short"/>
       <text macro="locator"/>
      </group>
     </else>
    </choose>
   </group>
  </layout>
 </citation>
 <bibliography subsequent-author-substitute="ders." line-spacing="1" hanging-indent="true">
  <sort>
   <key macro="sort-by-category"/>
   <key macro="sort-courts"/>
   <key macro="author"/>
   <key variable="issued"/>
  </sort>
  <layout suffix=".">
   <choose>
    <if type="article-journal">
     <group delimiter=", ">
      <text macro="author"/>
      <text variable="title"/>
      <group delimiter=" ">
       <text variable="container-title-short" suffix=" "/>
       <choose>
        <if match="any" variable="volume">
         <group delimiter=" ">
          <text variable="volume"/>
         </group>
         <group>
          <date date-parts="year" form="text" variable="issued" prefix="(" suffix="), "/>
          <text variable="page" prefix=" S. "/>
         </group>
        </if>
        <else-if match="any" variable="issue">
         <group delimiter="">
          <text variable="issue"/>
          <date date-parts="year" form="text" variable="issued" prefix="/"/>
          <text variable="page" prefix=", "/>
         </group>
        </else-if>
        <else>
         <group delimiter="">
          <date date-parts="year" form="text" variable="issued"/>
          <text variable="page" prefix=", "/>
         </group>
        </else>
       </choose>
      </group>
     </group>
     <choose>
      <text variable="DOI" prefix=", DOI: "/>
      <else>
       <group delimiter=" " prefix=", ">
        <text variable="URL" prefix="&lt;" suffix="&gt;, "/>
        <group delimiter=" ">
         <text term="accessed"/>
         <text macro="accessed-date"/>
        </group>
       </group>
      </else>
     </choose>
     <choose>
      <if match="any" variable="title-short">
       <group delimiter=", " prefix=" (zit.: " suffix=")">
        <text macro="autor-editor-note"/>
        <text variable="title-short"/>
       </group>
      </if>
     </choose>
    </if>
    <else-if type="book">
     <group delimiter=", ">
      <text macro="author"/>
      <text variable="title"/>
      <choose>
       <if match="any" variable="volume">
        <group delimiter=" ">
         <text term="volume"/>
         <text variable="volume"/>
        </group>
       </if>
      </choose>
      <text macro="collection-title"/>
      <text variable="edition" suffix=" Aufl."/>
      <group delimiter=" ">
       <text variable="publisher-place"/>
        <date date-parts="year" form="text" variable="issued"/>
      </group>
     </group>
     <choose>
      <if match="any" variable="title-short">
       <group delimiter=", " prefix=" (zit.: " suffix=")">
        <text macro="autor-editor-note"/>
        <text variable="title-short"/>
       </group>
      </if>
     </choose>
    </else-if>
    <else-if type="chapter">
     <group delimiter=", ">
      <text macro="author"/>
      <text variable="title"/>
      <group delimiter=": ">
       <text term="in"/>
       <group delimiter=", ">
        <names variable="editor">
         <name delimiter="/" name-as-sort-order="all" sort-separator=", " form="long"/>
         <label form="short" prefix=" (" suffix=")"/>
        </names>
        <text variable="container-title"/>
       </group>
      </group>
      <choose>
       <if match="any" variable="volume">
        <group delimiter=" ">
         <text term="volume"/>
         <text variable="volume"/>
        </group>
       </if>
      </choose>
      <text variable="edition" suffix=" Aufl."/>
      <group delimiter=" ">
       <text variable="publisher-place"/>
        <date date-parts="year" form="text" variable="issued"/>
      </group>
      <text variable="page" prefix="S. "/>
     </group>
     <choose>
      <if match="any" variable="title-short">
       <group delimiter=", " prefix=" (zit.: " suffix=")">
        <text macro="autor-editor-note"/>
        <text variable="title-short"/>
       </group>
      </if>
     </choose>
    </else-if>
    <else-if type="legal_commentary">
     <group delimiter=", ">
      <text macro="author"/>
      <choose>
       <if variable="title" match="all">
        <text variable="title" prefix="Kommentierung zu "/>
        <text macro="editor-author" prefix="in: "/>
       </if>          
      </choose>
      <text variable="container-title"/>
      <choose>
       <if match="any" variable="volume">
        <group delimiter=" ">
         <text term="volume"/>
         <text variable="volume"/>
        </group>
       </if>
      </choose>
      <text variable="edition" suffix=" Aufl."/>
      <group delimiter="">
       <text variable="publisher-place" suffix=" "/>
       <date variable="issued" suffix="">
        <date-part name="year" form="long"/>
       </date>
      </group>
     </group>
    </else-if>
    <else-if type="legal_case" match="any">
     <choose>
      <if type="legal_case" country="eu.int">
       <group delimiter=", ">
        <group delimiter=" ">
         <text value="EuGH"/>            
         <choose>
          <if variable="division">
           <text variable="division" prefix="(" suffix=")"/>
          </if>
         </choose>
        </group>
        <text variable="title" font-style="italic"/>
        <text variable="number" prefix="C-"/>
        <text variable="container-title"/>
        <text macro="issued-date-international-courts"/>            
        <text macro="locator"/>
       </group>
      </if>
      <else-if type="legal_case" country="coe.int">
       <group delimiter=", ">
        <group delimiter=" ">
         <text value="EGMR"/>
         <choose>
          <if variable="division">
           <text variable="division" prefix="(" suffix=")"/>
          </if>
         </choose>
        </group>
        <group delimiter=" ">
         <text variable="title" font-style="italic"/>
         <text variable="note" prefix="(" suffix=")"/>
        </group>
        <text variable="number" prefix="Nr. "/>
        <text macro="issued-date-international-courts"/>
        <text macro="locator"/>
       </group>
      </else-if>
      <else-if type="legal_case" country="un.int">
       <group delimiter=", ">
        <text value="IGH"/>
        <group delimiter=" ">
         <text variable="title" font-style="italic"/>
         <text variable="title-short" prefix="(" suffix=")"/>
        </group>
        <text variable="genre"/>
        <text macro="issued-date-international-courts"/>
        <text variable="container-title"/>
       </group>
      </else-if>
      <else-if type="legal_case" country="ch">
       <group delimiter=", ">
        <group delimiter=" ">
         <text variable="authority"/>
         <text variable="title"/>
         <text variable="number"/>
         <text macro="issued-date"/>
        </group>
        <text variable="container-title"/>
        <text variable="page" prefix="S. "/>
        <text variable="title-short" font-style="italic"/>
       </group>
      </else-if>
      <else>
       <group delimiter=", ">
        <text variable="authority"/>
        <text variable="title"/>
        <text macro="issued-date"/>
        <text variable="note"/>
        <text variable="title-short" font-style="italic" prefix="&#8211; "/>
       </group>
      </else>
     </choose>
    </else-if>        
    <else-if type="document">
     <group delimiter=", ">
      <group delimiter=" ">
       <text variable="title"/>
       <text macro="issued-date"/>
      </group>
      <text variable="publisher"/>
      <group delimiter=" ">
       <text value="abrufbar unter"/>
       <text variable="URL" prefix="&lt;" suffix="&gt;"/>
      </group>
      <group delimiter=" ">
       <text term="accessed"/>
       <text macro="accessed-date"/>
      </group>
     </group>
    </else-if>      
    <else-if type="hearing">
     <group delimiter=" ">
      <text variable="title" suffix=", "/>
      <text variable="publisher"/>
      <text macro="issued-date-hearing"/>
      <text variable="authority"/>
      <text variable="page"/>
     </group>
    </else-if>
    <else-if type="report">
     <group delimiter=", ">
      <text macro="author"/>
      <text variable="title"/>
      <date variable="issued" suffix="">
        <date-part name="year" form="long"/>
      </date>
      <text variable="page"/>
     </group>
    </else-if>
    <else>
     <group delimiter=", ">
      <text macro="author"/>
      <text variable="title"/>
      <group delimiter=" ">
       <text variable="URL" prefix="&lt;" suffix="&gt; "/>
       <group delimiter=" " prefix="(" suffix=")">
        <text term="accessed"/>
        <text macro="accessed-date"/>
       </group>
       <text variable="page"/>
       <text variable="title-short" prefix=" (zit.: " suffix=")"/>
      </group>
     </group>
    </else>
   </choose>
  </layout>
 </bibliography>
</style>
